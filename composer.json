{
  "name": "goldeagle/takoff",
  "description": "a group of tools such as lodash, using php",
  "keywords": [
    "lodash",
    "utility",
    "utilities",
    "php-library",
    "utility-library",
    "php-utility"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "goldeagle",
      "email": "1308362@gmail.com"
    }
  ],
  "autoload": {
    "files": [
      "src/bootstrap.php"
    ]
  },
  "require": {
    "php": "^7.1",
    "sebastian/comparator": "^1.2 | ^2.0 | ^2.1 | ^3.0",
    "symfony/property-access": "^2.7 | ^3.0 | ^4.0",
    "ext-json": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpdocumentor/reflection-docblock": "^4.2",
    "bamarni/composer-bin-plugin": "^1.2"
  },
  "scripts": {
    "post-install-cmd": [
      "@composer bin all install --ansi"
    ],
    "post-update-cmd": [
      "@composer bin all update --ansi"
    ]
  }
}
